<h1>Results Page!!!</h1>

<% if (data["Response"] === "True") { %>
  <ul>
    <% data["Search"].forEach(function(movie) { %>
      <li>
          <strong>
              <%= movie["Title"] %>
          </strong> 
          - <%=movie["Year"]%>
      </li>
    <% }) %>
  </ul>
<% } else { %>
  <p>No Data Found :(</p>
  <% } %>
<a href="/">Search Again!</a>

